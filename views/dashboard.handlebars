<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Your Queue</h3>
  </div>

			<div id="content-panel" class="panel-heading">
				<h3 class="panel-title">YouTube</h3>

					<div class="panel-body">
						<div id="Twitch" class="content carousel">
						{{#each content}}
						<div>
							<a href="https://www.youtube.com/watch?v={{this.id.videoId}}" target="_blank"><img src={{this.snippet.thumbnails.medium.url}}></img></a>
							<div class="caption" style="color: black">
								<h5>{{this.snippet.title}}</h5>
							</div>
						</div>
						{{/each}}
						</div>
					</div>
				</div>

