<div id="carousel" class="content">
{{#each content}}
<div>
	<a href="https://www.youtube.com/watch?v={{this.id.videoId}}"><img src={{this.snippet.thumbnails.medium.url}}></img></a>
	<h5>{{this.snippet.title}}</h5>
</div>
{{/each}}
</div>
{{#if message}}
  <script>
    showDialog("{{message}}"); // /javascripts/common.js
  </script>
{{/if}}
